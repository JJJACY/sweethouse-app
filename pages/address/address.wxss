.checkbox{
  position: relative;
  display: flex;
  padding: 14rpx 80rpx;
  border-bottom: 2rpx solid #e7e7e7;
}
.box{
  content: "\20";
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: 50%;
  left: 20rpx;
  margin-top: -20rpx;
}
.userinfo{
  
}
.info{
  display: flex;
  margin-top: 8rpx;
  list-style: none;
  font-size: 32rpx;
  color:#333;
}
.usertel{
  margin-left:10rpx;
}
.useraddress{
  display: flex;
}
.default{
  display: inline-block;
  /* position: relative;
  top: 10rpx; */
  width: 60rpx;
  height: 30rpx;
  padding: 0 10rpx;
  margin: 10rpx 10rpx 0 0;
  vertical-align: middle;
  line-height: 30rpx;
  font-size: 20rpx;
  color: #f2270c;
  border: 2rpx solid #f2270c;
}
.address{
  height: 84rpx;
  font-size: 28rpx;
  color: #666;
  word-break: break-all;
}
.edit{
  position: relative;
  right: -40rpx;
  top: 40rpx;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  color: #f2270c;
  font-size: 28rpx;
  z-index: auto;
}
.create{
  margin: 40rpx;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
  color: #666666;
  border: 2px solid #ccc;
  box-shadow: 0 6rpx 12rpx 0 rgba(0,0,0,.05);
  z-index: 1;

}