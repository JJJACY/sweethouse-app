.container{
  /* display: block; */
}
.notic{
  width: 702rpx;
  padding: 16rpx 24rpx;
  color: #f56723;
  font-size: 24rpx;
  line-height: 1.5;
  background-color: #fff7cc;
}
.address-list{
  flex: 1;
  width: 100%;
}
.address-detail{
  display: flex;
  flex-direction: column;
  flex-wrap: wrap;
  font-size: 26rpx;
  color: #666;
  line-height: 1.4;
  height: auto;
  padding: 15rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 2prx solid #fff;
}
.address-top {
  display: flex;
}
.address-buttom{
  display: flex;
}
.address-located{
  color: #000;
}
.btn{
  display: flex;
  height: 100%;
  width: 200rpx;
}
.del-btn{
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #fff;
  line-height: 100rpx;
  background-color: #ee0a24;
  border: 2rpx solid #ee0a24;
}
.edit-btn{
  padding: 0 20rpx;
  font-size: 28rpx;
  color: #fff;
  line-height: 100rpx;
  background-color: #07c160;
    border: 2rpx solid #07c160;
}
.create{
  width: 90%;
  margin: 40rpx;
  font-size: 32rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
  color: #666666;
  border: 2rpx solid #ccc;
  box-shadow: 0 6rpx 12rpx 0 rgba(0,0,0,.05);
  z-index: 1;

}